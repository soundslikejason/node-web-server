{
    "rules": {
        "indent": [
            "warn",
            2
        ],
        "semi": [
            "error",
            "always"
        ],
        "no-console": "warn"
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "extends": "eslint:recommended"
}